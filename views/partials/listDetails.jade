header
	a.header-button.icon(href="/list")
			div back
	h1#header Name of List
	div.header-menu-bar.right
		a.header-menu-button.icon(href="/list/1/option")
			span
				i.icon-cog.icon-2x
		a.header-menu-button.icon(href="/list/1/additem")
			span
				i.icon-plus.icon-2x
div.stp-content-frame
	div.stp-content-body.container
		.span12
			ul.row-fluid
				li.span6(ng-repeat="n in [1,2,3,4,5,6]")
					div.list-container(ng-click="ListItemDetail()")
						div.row
							div.left.list-name
								| Item Name {{n}}
							div.right
								i.icon-chevron-right
	