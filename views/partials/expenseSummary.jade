header
	a.header-button.icon.menu(href="javascript:void(0)",ng-click="menuClick()")
		span Menu
	h1#header My Expense Summary

				

div.stp-content-frame
	div.stp-content-body
		div.row.dashboard
			.six.columns.mobile-two
				.header
					| I OWE
				.well.single-value.selectable
					| $100.00
					h5
						small
						a(href="expense/details/1")
							i.icon-eye-open
							|  view details
			.six.columns.mobile-two
				.header
					| FRIENDS OWE
				.well.single-value.selectable
					| $50.00
					h5
						small
						a(href="/expense/friends")
							i.icon-eye-open
							|  view details
		div.row
			// My list of activities
			.twelve.columns.mobile-four
				div.listHeader
					| Recent Activity
		div.row
			ul.block-grid.two-up.mobile
				li(ng-repeat="list in userExpense")
					div.list-container.selectable(ng-click="",style="padding:5px")
						div.row.expense-row
							div.mobile5-five.columns.eight
								h5
									| {{list.payer}} 
									small
										| has paid {{list.payee}} ${{list.amount}} for {{list.period}} on {{list.date}}

	